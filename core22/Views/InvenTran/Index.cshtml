@model IEnumerable<core22.Models.POS.Models.InvenTran>

@{
    ViewData["Title"] = "Inventory Transactions";
}

<h1>@ViewData["Title"]</h1>


<table class="table" id="grid">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TranSourceNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TranSourceType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvenTranDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Product.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TranSourceNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TranSourceType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Quantity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InvenTranDate)
                </td>
            </tr>
        }
    </tbody>
</table>


@section Scripts{
    <script>
        $(document).ready(function () {
            $('#grid').DataTable();
        });
    </script>
}